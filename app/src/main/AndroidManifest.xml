<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yandex.job">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".AuthActivity"></activity>
        <activity android:name=".AutoparkActivity" />
        <activity android:name=".Registration4Activity" />
        <activity android:name=".Registration3Activity" />
        <activity android:name=".Registration2Activity" />
        <activity android:name=".Registration1Activity" />
        <activity
            android:name=".LoggedOutActivity"
            android:theme="@style/AppTheme.CustomToolbar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>